{
  "name": "permalindo-manager",
  "version": "1.0.6",
  "private": true,
  "main": "index.js",
  "keywords": [],
  "author": "flumi",
  "license": "ISC",
  "scripts": {
    "use:pnpm": "npx only-allow pnpm",
    "use:yarn": "npx only-allow yarn",
    "de:npm": "npx npkill",
    "pnpm:prep": "pnpm i && pnpm import",
    "clean": "git clean -xfd && git submodule foreach --recursive git clean -xfd && yarn",
    "gradle:clean": "cd android && ./gradlew clean && cd ..",
    "android": "react-native run-android",
    "android:testrelease": "react-native run-android --mode release",
    "android:release": "npx react-native build-android --mode=release",
    "ios:prep": "bundle install && npx pod-install && rm -f ./ios/.xcode.env.local",
    "ios": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --mode Debug",
    "ios:release": "react-native run-ios --simulator=\"iPhone 15 Pro Max\" --mode Release",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file=index.js --bundle-output ios/main.jsbundle",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "versioning": "npx react-native-version",
    "android:go": "yarn bundle:android && yarn android:release",
    "lint": "npx eslint .",
    "start": "react-native start",
    "start:newcache": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@animatereactnative/marquee": "^0.2.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/auth": "^20.4.0",
    "@react-native-firebase/firestore": "^20.4.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/core": "^6.4.16",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.5",
    "appcenter": "^5.0.1",
    "appcenter-analytics": "^5.0.1",
    "appcenter-crashes": "^5.0.1",
    "axios": "^1.7.2",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "lottie-react-native": "^6.7.2",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-bootsplash": "^5.5.3",
    "react-native-bouncy-checkbox": "^4.0.1",
    "react-native-color-matrix-image-filters": "^6.0.9",
    "react-native-date-picker": "^5.0.4",
    "react-native-device-info": "^11.1.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-image-crop-picker": "^0.41.1",
    "react-native-mmkv-storage": "^0.10.3",
    "react-native-reanimated": "^3.12.0",
    "react-native-rss-parser": "^1.5.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^4.5.0",
    "react-native-svg": "^15.4.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
